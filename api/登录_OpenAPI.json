{
  "openapi": "3.0.1",
  "info": {
    "title": "Funny Toolbox API",
    "description": "Funny Toolbox 平台后端接口文档，包含插件管理、菜单等模块",
    "contact": {
      "name": "hxuanyu",
      "url": "https://github.com/hxuanyu",
      "email": ""
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "v1.0.0"
  },
  "servers": [
    {
      "url": "/",
      "description": "默认服务"
    }
  ],
  "paths": {
    "/api/auth/login": {
      "post": {
        "tags": [
          "认证"
        ],
        "summary": "登录",
        "description": "使用 application.yaml 配置的账号密码登录",
        "operationId": "login",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ResultVoid"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "LoginRequest": {
      "type": "object",
      "properties": {
        "username": {
          "type": "string",
          "refType": null
        },
        "password": {
          "type": "string",
          "refType": null
        }
      }
    },
    "ResultVoid": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "data": {
          "type": "object"
        }
      }
    }
  }
}