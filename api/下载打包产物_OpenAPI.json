      "get": {
        "tags": [
          "平台管理"
        ],
        "summary": "下载打包产物",
        "description": "下载 /data/builds/frontend-plugins 目录下的打包 ZIP 文件",
        "operationId": "downloadPackedZip",
        "parameters": [
          {
            "name": "file",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "type": "string",
                  "format": "byte"
                }
              }
            }
          }
        }
      }
    }
  },
